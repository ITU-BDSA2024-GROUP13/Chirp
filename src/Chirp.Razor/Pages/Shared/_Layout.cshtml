<!DOCTYPE html>
<html lang="en">
<head>
    <title>@ViewData["Title"]</title>
    <link href="/css/style.css" rel="stylesheet" type="text/css" />
    <link rel="icon" type="image/png" href="/favicon/favicon.ico">
</head>
<body>
    <box onclick = "window.location = '/'" class = "bird">
        <img src="/images/icon1.png" class = "Icon" alt="Icon1"/>
    </box>
    <h1 class = "centerbox">
        Chirp
    </h1>
    <div class = "Navigation">
        <table>
                <tr onclick = "window.location = '/'">
                    <td><font size = "+2"> Home</font></td>
                </tr>
                
                <th onclick = "window.location = 'favoriteW.html'"> 
                    <td><font size = "+2"> Browse </font></td> 
                <th>
    
                <tr onclick = "window.location = 'more.html'">
                   <td><font size = "+2"> More </font></td>
                </tr>
    
                <th onclick = "window.location = 'Log in.html'">
                    <font size = "+2"> Log in </font> 
                </th>
            
        </table>
    </div>
    <div class=page>
        <div class=body>
            @RenderBody()
        </div>
        <div class=footer>
            Chirp &mdash; An ASP.NET Application
        </div>
    </div>
</body>
@await RenderSectionAsync("Scripts", required: false)
</html>
