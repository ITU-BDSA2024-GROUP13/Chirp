{
  "ConnectionStrings": {
    "DefaultConnection": "Server=tcp:your-sql-server-name.database.windows.net,1433;Database=your-database-name;User Id=username;Password=password;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning"
    },
    "AzureAppServices": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Https": {
        "Url": "https://*:443"
      }
    },
    "RedirectToHttps": true
  },
  "ApplicationInsights": {
    "InstrumentationKey": "YOUR-INSTRUMENTATION-KEY"
  },
  "AllowedHosts": "*",
  "HealthCheck": {
    "HealthEndpoint": "/health"
  }
}
